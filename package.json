{
  "name": "uniconvert-monorepo",
  "version": "1.2.2",
  "private": true,
  "description": "A universal file converter for images, videos, audio and documents",
  "scripts": {
    "dev": "pnpm --filter @uniconvert/electron-app dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "release": "pnpm --filter @uniconvert/electron-app release",
    "release:build": "pnpm --filter @uniconvert/electron-app release:build",
    "release:publish": "pnpm --filter @uniconvert/electron-app release:publish",
    "release:organize": "node scripts/organize-release.js",
    "prepare:icons": "node scripts/gen-icon.js"
  },
  "keywords": [],
  "author": "0png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0png/uniconverter.git"
  },
  "devDependencies": {
    "fast-check": "^4.5.3",
    "vitest": "^4.0.17"
  }
}
